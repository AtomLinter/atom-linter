{
  "name": "atom-linter",
  "version": "10.0.0",
  "description": "Helper module for atom linter providers",
  "main": "lib/index.js",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "format": "prettier ./src --write",
    "lint": "eslint .",
    "test": "npm run compile && atom --test spec",
    "clean": "rimraf lib",
    "compile": "npm run clean && tsc -p src/tsconfig.json ",
    "watch": "npm run compile --watch",
    "prepare": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/steelbrain/atom-linter.git"
  },
  "keywords": [
    "atom",
    "linter"
  ],
  "author": "steelbrain",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/steelbrain/atom-linter/issues"
  },
  "homepage": "https://github.com/steelbrain/atom-linter#readme",
  "dependencies": {
    "grim": "^2.0.3",
    "named-js-regexp": "^1.3.5",
    "sb-exec": "^4.0.0",
    "tmp": "~0.2.1"
  },
  "devDependencies": {
    "@types/atom": "^1.40.10",
    "@types/jasmine": "^3.6.7",
    "eslint": "^7.22.0",
    "eslint-config-atomic": "^1.12.2",
    "jasmine-fix": "^1.3.1",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.2.3"
  }
}
